{
  "root": "apps/rio-de-janeiro",
  "sourceRoot": "apps/rio-de-janeiro",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/rio-de-janeiro",
        "outputPath": "dist/apps/rio-de-janeiro"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "rio-de-janeiro:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "rio-de-janeiro:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "rio-de-janeiro:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "rio-de-janeiro:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/rio-de-janeiro"],
      "options": {
        "jestConfig": "apps/rio-de-janeiro/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/rio-de-janeiro/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
